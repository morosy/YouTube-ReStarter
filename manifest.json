{
    "manifest_version": 3,
    "name": "YouTube Always Start at 0:00",
    "version": "1.0.0",
    "description": "YouTubeの動画を必ず0:00から再生します",
    "permissions": [],
    "host_permissions": [
        "https://www.youtube.com/*"
    ],
    "content_scripts": [
        {
            "matches": [
                "https://www.youtube.com/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_start"
        }
    ]
}
